  "version": "1.0.0",
  "description": "Indexing example components and scripts",
  "main": "manifest.yml",
  "repository": "git@github.com:flair-sdk/starter-boilerplate.git",
  "author": "gm@flair.dev",
  "license": "MIT",
  "private": true,
  "scripts": {
    "format": "rome format . --write",
